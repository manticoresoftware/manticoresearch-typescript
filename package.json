{
  "name": "manticoresearch-ts",
  "version": "4.1.0",
  "description": "Typescript client for Manticore Search",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "test": "cross-env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'test/**/*.test.ts'",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "browser": {
    "fs": false
  },
  "keywords": [
    "manticoresearch",
    "typescript",
    "REST",
    "search",
    "client",
    "index"
  ],  
  "peerDependencies": {
    "typescript": ">=4.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },  
  "devDependencies": {
    "@types/chai": "^4.3.9",
    "@types/jest": "^29.5.6",
    "@types/mocha": "^10.0.3",
    "chai": "^4.3.10",
    "cross-env": "^7.0.3",
    "mocha": "^10.2.0",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typescript": "^4.0"
  }
}
